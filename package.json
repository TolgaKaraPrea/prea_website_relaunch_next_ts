{
  "name": "prea_relaunch_nextjs",
  "version": "0.1.4",
  "description": "PREA website relaunch with Typescript.",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,tsx,json,md}": [
      "eslint — fix"
    ],
    "**/*": "prettier — write — ignore-unknown"
  },
  "scripts": {
    "dev": "next dev -p 3330",
    "build": "rm -rf ./build && next build",
    "buildBefore": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start",
    "serve": "next start",
    "fix": "yarn run lint -- --fix & prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "next lint",
    "lint:fix": "yarn run lint -- --fix",
    "lint:css": "npx stylelint \"**/style.ts\" ",
    "prisma:generate": "prisma generate",
    "make": "plop --plopfile generators/plopfile.js",
    "make:template": "plop --plopfile generators/controllers/templateController.js",
    "make:constant": "plop --plopfile generators/controllers/constantController.js",
    "make:component": "plop --plopfile generators/controllers/componentController.js",
    "make:page": "plop --plopfile generators/controllers/pageController.js",
    "test:build": "next lint & next build",
    "test": "jest --watch",
    "test:unit": "jest --config jest.config.js --env=jsdom",
    "placeholder": "node ./src/utils/javascript/createPlaceholder.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "update-translations": "extract-messages -l=de -o locales/de -d de --flat false \"internationalization/**/!(*.test).js\"",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public -o docs-build",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
